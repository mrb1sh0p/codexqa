{
  "manifest_version": 3,
  "name": "codex.qa",
  "version": "1.0",
  "description": "Captura uma pergunta da tela e envia para an√°lise.",
  "permissions": ["contextMenus", "tabs", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "128": "icon.png"
  }
}
